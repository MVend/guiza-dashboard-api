{
  "name": "feasummit-back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --require babel-core/register --require babel-polyfill ./src/server.js",
    "start:prod": "npm run drop:db && npm run migrate && npm run seed && node --require babel-core/register --require babel-polyfill ./src/server.js",
    "dev": "nodemon --require babel-core/register --require babel-polyfill ./src/server.js",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "drop:db": "sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patricktunez123/FEASummit-back-end.git"
